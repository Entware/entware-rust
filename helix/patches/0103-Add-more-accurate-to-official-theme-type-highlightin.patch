Subject: [PATCH 103/188] Add more accurate to official theme type highlighting
 to Darcula themes (#8738)

---
 runtime/themes/darcula.toml | 6 +++++-
 1 file changed, 5 insertions(+), 1 deletion(-)

--- a/runtime/themes/darcula.toml
+++ b/runtime/themes/darcula.toml
@@ -35,7 +35,11 @@
 "constant.numeric" = "lightblue"
 "constant.character.escape" = "white"
 "attribute" = "yellow"
-"type" = "orange"
+"type" = "white"
+"type.builtin" = "orange"
+"type.parameter" = { fg = "white", modifiers = ["bold"] }
+"type.enum" = "white"
+"type.enum.variant" = "orange"
 "string"  = "darkgreen"
 "function" = "yellow"
 "function.macro" = "green"
